      "item": {
        "type": "markdown",
        "id": "bb230533c837f23a",
        "text": "Hi Matthew"
      },
      "date": 1502351100450
    },
    {
      "item": {
        "type": "factory",
        "id": "f3bfcd0fb63bc028"
      },
      "id": "f3bfcd0fb63bc028",
      "type": "add",
      "after": "bb230533c837f23a",
      "date": 1502351176352
    },
    {
